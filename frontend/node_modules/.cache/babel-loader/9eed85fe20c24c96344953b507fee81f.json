      </el-form-item>\n    </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n\tdata(){\n\t\treturn {\n\t\t\t// 登录表单绑定的对象\n\t\t\tregisterForm: {\n\t\t\t\tusername: '',\n\t\t\t\tpassword: '',\n\t\t\t\tnickname: '',\n\t\t\t},\n\t\t\t// 使用登录表单验证\n\t\t\tloginFormRules: {\n\t\t\t\tusername: [\n\t\t\t\t\t{required: true, message: \"ユーザーネームを入力してください\", trigger: \"blur\" },\n\t\t\t\t\t{min: 3, max:10, message: \"长度在3到10个字符之间\", trigger: \"blur\"}\n\t\t\t\t],\n\t\t\t\tpassword: [\n\t\t\t\t\t{required: true, message: \"パスワードを入力してください\", trigger: \"blur\"},\n\t\t\t\t\t{min:5, max:16, message: \"长度在5到16个字符之间\", trigger: \"blur\"}\n\t\t\t\t],\n\t\t\t\tnickname: [\n\t\t\t\t\t{required: true, message: \"名前を入力してください\", trigger: \"blur\"},\n\t\t\t\t\t{min:2, max:16, message: \"长度在2到16个字符之间\", trigger: \"blur\"}\n\t\t\t\t],\n\t\t\t}\n\t\t}\n\t},\n\tmethods: {\n\t\t// 重置表单方法, method里面的this指向的是这个组件的实例\n\t\tresetRegisterForm(){\n\t\t\t// $代表着vue定义的属性,用来和用户定义的属性相互区分\n\t\t\tthis.$refs.registerFormRef.resetFields();\n\t\t},\n\t\tregister(){\n\t\t\tthis.$refs.registerFormRef.validate(async valid=> {\n\t\t\t\tif(!valid){\n\t\t\t\t\treturn;\n\t\t\t\t}else{\n\t\t\t\t\tthis.authentication()\n\t\t\t\t}\n\t\t\t\t\n\t\t\t})\n\t\t},\n\t\tasync authentication(){\n\t\t\tconst {data:res} = await this.$http.post('login/register', this.registerForm);\n\t\t\tif(res.code!==200){\n\t\t\t\tthis.$message.error('ユーザー登録が失敗しました、改めて登録してください');\n\t\t\t}else{\n\t\t\t\tthis.$message.success('ユーザー登録しました！');\n\t\t\t\t// 跳转到home里\n\t\t\t\tthis.$router.push('/login')\n\t\t\t}\n\t\t}\n\t}\n};\n</script>\n\n// scope 限制样式作用域，仅在本组件\n<style scoped>\n.login_container {\n  background-color: #2b4b6b;\n  height: 100%;\n}\n.login_box {\n  width: 450px;\n  height: 300px;\n  background-color: #fff;\n  border-radius: 3px;\n  position: absolute;\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n}\n.avatar_box {\n  height: 130px;\n  width: 130px;\n  border-radius: 50%;\n  position:absolute;\n  left:50%;\n  transform: translate(-50%, -50%);\n  background-color: #fff;\n}\n.avatar_box > img {\n  width: 100%;\n  height: 100%;\n  border-radius: 50%;\n  background-color: #eee;\n}\n.login_form{\n  position:absolute;\n  bottom: 0;\n  width: 100%;\n  padding: 0 20px;\n  box-sizing: border-box;\n}\n</style>\n"],"sourceRoot":"src/views/login"}]}