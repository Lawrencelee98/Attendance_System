=\"nickname\" >\r\n\t    <el-input prefix-icon='el-icon-apple' v-model='registerForm.nickname'></el-input>\r\n\t  </el-form-item>\r\n      <!-- 按钮 -->\r\n      <el-form-item >\r\n        <el-button type='primary' @click='register'>ユーザー登録</el-button>\r\n        <el-button type='info' @click=\"resetRegisterForm\">リセット</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\tdata(){\r\n\t\treturn {\r\n\t\t\t// 登录表单绑定的对象\r\n\t\t\tregisterForm: {\r\n\t\t\t\tusername: '',\r\n\t\t\t\tpassword: '',\r\n\t\t\t\tnickname: '',\r\n\t\t\t},\r\n\t\t\t// 使用登录表单验证\r\n\t\t\tloginFormRules: {\r\n\t\t\t\tusername: [\r\n\t\t\t\t\t{required: true, message: \"ユーザーネームを入力してください\", trigger: \"blur\" },\r\n\t\t\t\t\t{min: 3, max:10, message: \"长度在3到10个字符之间\", trigger: \"blur\"}\r\n\t\t\t\t],\r\n\t\t\t\tpassword: [\r\n\t\t\t\t\t{required: true, message: \"パスワードを入力してください\", trigger: \"blur\"},\r\n\t\t\t\t\t{min:5, max:16, message: \"长度在5到16个字符之间\", trigger: \"blur\"}\r\n\t\t\t\t],\r\n\t\t\t\tnickname: [\r\n\t\t\t\t\t{required: true, message: \"名前を入力してください\", trigger: \"blur\"},\r\n\t\t\t\t\t{min:2, max:16, message: \"长度在2到16个字符之间\", trigger: \"blur\"}\r\n\t\t\t\t],\r\n\t\t\t}\r\n\t\t}\r\n\t},\r\n\tmethods: {\r\n\t\t// 重置表单方法, method里面的this指向的是这个组件的实例\r\n\t\tresetRegisterForm(){\r\n\t\t\t// $代表着vue定义的属性,用来和用户定义的属性相互区分\r\n\t\t\tthis.$refs.registerFormRef.resetFields();\r\n\t\t},\r\n\t\tregister(){\r\n\t\t\tthis.$refs.registerFormRef.validate(async valid=> {\r\n\t\t\t\tif(!valid){\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}else{\r\n\t\t\t\t\tthis.authentication()\r\n\t\t\t\t}\r\n\t\t\t\t\r\n\t\t\t})\r\n\t\t},\r\n\t\tasync authentication(){\r\n\t\t\tconst {data:res} = await this.$http.post('login/register', this.registerForm);\r\n\t\t\tif(res.code!==200){\r\n\t\t\t\tthis.$message.error('ユーザー登録が失敗しました、改めて登録してください');\r\n\t\t\t}else{\r\n\t\t\t\tthis.$message.success('ユーザー登録しました！');\r\n\t\t\t\t// 跳转到home里\r\n\t\t\t\tthis.$router.push('/login')\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n};\r\n</script>\r\n\r\n// scope 限制样式作用域，仅在本组件\r\n<style scoped>\r\n.login_container {\r\n  background-color: #2b4b6b;\r\n  height: 100%;\r\n}\r\n.login_box {\r\n  width: 450px;\r\n  height: 300px;\r\n  background-color: #fff;\r\n  border-radius: 3px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n.avatar_box {\r\n  height: 130px;\r\n  width: 130px;\r\n  border-radius: 50%;\r\n  position:absolute;\r\n  left:50%;\r\n  transform: translate(-50%, -50%);\r\n  background-color: #fff;\r\n}\r\n.avatar_box > img {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 50%;\r\n  background-color: #eee;\r\n}\r\n.login_form{\r\n  position:absolute;\r\n  bottom: 0;\r\n  width: 100%;\r\n  padding: 0 20px;\r\n  box-sizing: border-box;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}